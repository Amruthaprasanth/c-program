#include <stdio.h>

int main()
{
    int arr1[10],arr2[10],n1,n2,arr3[20];
    printf("enter the size of first array:");
    scanf("%d",&n1);
    printf("enter %d elements",n1);
    for(i=0;i<n1;i++)
    scanf("%d",&arr1[i]);
    printf("enter the size of second array:");
    scanf("%d",&n2);
    printf("enter %d elements",n2);
    for(i=0;i<n2;i++)
    scanf("%d",&arr2[i]);
    for(i=0;i<n1;i++)
    arr3[i]=arr1[i];
    return 0;
}